! Swap the n! Swap the number row with their shifted values for a German keyboard layout and additional custom bindings

! Key bindings for number row
keycode  10 = exclam 1 exclam 1
keycode  11 = quotedbl 2 quotedbl 2 twosuperior
keycode  12 = parenleft 3 parenleft 3 threesuperior
keycode  13 = parenright 4 dollar parenright dollar
keycode  14 = bracketleft 5 percent bracketleft percent
keycode  15 = bracketright 6 ampersand bracketright ampersand
keycode  16 = slash 7 slash 7 backslash
keycode  17 = braceleft 8 braceleft 8
keycode  18 = braceright 9 braceright 9
keycode  19 = equal 0 equal 0


! Swap ? and ÃŸ
keycode 20 = question ssharp
keycode 49 = ssharp question

! Define Alt_R as Mode_switch
keycode 225 = Mode_switch

remove lock = Caps_Lock
keysym Caps_Lock = Super_L
